{
  "name": "template-assistant",
  "version": "1.0.0",
  "description": "This is a template repository to build assistants/ agents for future projects. This repository should have all the requirements, from branch access control to setup files, \r required for smooth initiation, testing of future projects with an automated system of CI/CD with Github Releases and Actions.",
  "main": "index.js",
  "scripts": {
    "test:fe": "jest --config jest.config.js",
    "prepare": "husky install",
    "prettier-check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "prettier-fix": "prettier --write ."
  },
  "lint-staged": {
