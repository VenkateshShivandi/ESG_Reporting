    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.6",
    "@types/testing-library__react": "^10.0.1",
    "eslint": "^9.18.0",
    "eslint-plugin-react": "^7.37.4",
    "flake8": "^0.0.1",
    "globals": "^15.14.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.1",
