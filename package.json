      "flake8",
      "black"
    ],
    "*.js": [
      "eslint ."
    ]
  },
  "eslintConfig": {
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
